# MANIFEST.in
# This tells distutils what files to distribute.

global-exclude *.pyc
global-exclude *~

include LICENSE.txt
include README.md
include MANIFEST.in
include setup.py
include requirements.txt
include ypkpathway.png
include icon.png

recursive-include tests *
recursive-include docs *
